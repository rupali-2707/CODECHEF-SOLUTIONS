# --------------------------------METHOD1-----------------------------------

#include <bits/stdc++.h> 

using namespace std;

int main() {
// 	ios_base::sync_with_stdio(false);
// 	cin.tie(NULL);

// 	// Read the input n, k
// 	int n, k;
// 	cin >> n >> k;

// 	// ans denotes number of integers n divisible by k
// 	int ans = 0;

// 	for (int i = 0; i < n; i++) {
// 		int t;
// 		cin >> t;
		
// 		if (t % k == 0) {
// 			ans++;
// 		}		
// 	}

// 	// Print the ans.
// 	cout << ans << "\n";	
		
		int n,k,count=0;
		
		cin>>n>>k;
		int arr[n];
		
		for(int i=0;i<n;i++)
		{
		cin>>arr[i];
		}
		for(int i=0;i<n;i++)
		{
		if(arr[i] % k ==0)
		{
		    count++;
		}
		}
		cout << count;
	return 0;
}



#-------------------------------------------------METHOD2--------------------------------------

#include <bits/stdc++.h> 

using namespace std;

int main() {

		
		int n,k,count=0;
		
		cin>>n>>k;
		int arr[n];
		
		for(int i=0;i<n;i++)
		{
		cin>>arr[i];
		if(arr[i] % k ==0)
		{
 		    count++;
 		}
		}

		cout << count;
	return 0;
}
